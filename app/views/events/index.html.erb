<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<table>
  <thead>
    <tr>
      <td>Titulo</td>
      <td>Fecha</td>
      <td>Usuario</td>
      <td>Creado en</td>
      <td>Actualizado en</td>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
        <tr>
          <td><%= event.title %></td>
          <td><%= event.date %></td>
          <td><%= event.user_id %></td>
          <td><%= event.created_at %></td>
          <td><%= event.updated_at %></td>
        </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Nuevo Event', new_event_path %>
